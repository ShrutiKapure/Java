import java.util.*;
public class Extract_Digit {

    
    public static void main(String[] args) {
        int y, i=0;
        String z = "";
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter Year:");
        int x;
        x = sc.nextInt();
        while(i<2){
            y = x % 10;
            z = y + "" + z;
            x = x / 10;
            i++;
        }
        System.out.println("Last two digits:"+z);
    }
    
}
