public class Main {

    //creating a method
    public int addNumbers(int a, int b){
        int sum = a + b;
        return sum;
    }
    public static void main(String[] args) {
      int num1 = 5;
      int num2 = 4;
      
      //create an object of main
      Main obj = new Main();
      
      //call the method
      int result = obj.addNumbers(num1, num2);
      System.out.println("Sum is " +result);
      
    }
    
}
