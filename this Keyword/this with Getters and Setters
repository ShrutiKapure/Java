class Main {
    String name;
    
    void setName(String name){
        this.name = name;
    }
    String getName(){
        return this.name;
    }

    public static void main(String[] args) {
        Main obj = new Main();
        
        obj.setName("Shruti");
        System.out.println("obj.name : " + obj.getName());
        
        
    }
}
